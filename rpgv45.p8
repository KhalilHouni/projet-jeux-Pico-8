--mise a jour de l'ennemi
function draw_enemies()
	for e in all(enemies) do
		spr(127,e.x,e.y)
	end
end	
		

function colision_player()
	return 
				e.x < p.x + p.width and
				e.x + e.width > p.x and
				e.y < p.y + p.height and
				e.y + e.height > p.y
end
