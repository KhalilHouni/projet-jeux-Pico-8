--declaration des variables
local sword = {
    active = false,
    sprite = 80,
    duration = 0.15,
    length = 16,
    hitboxsize = 8
}

local inventory = {
    visible = false,
    menuy = 64, -- position verticale du menu inventaire (centre de l'れたcran)
    menuheight = 60 -- hauteur du menu inventaire
}


-- initialisation
function _init()
    create_player()
    enemies={}
    init_msg()
    init_camera()
    
end

-- mise a jour
function _update()

    if #messages == 0 then
        player_movement()
        update_sword()
        inventory.update()
    end
    
    if #enemies == 0 then
    	spawn_enemies()
    	
    end
   --update_enemies()
   	
    update_camera()
    update_msg()

end

-- draw
function _draw()
    cls()
    draw_map()
  	 draw_player() 	 
    draw_sword()
    draw_enemies()
    draw_ui()
    draw_msg()
    inventory.draw()
end

